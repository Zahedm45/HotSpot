<?xml version="1.0" encoding="UTF-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >
    <item>
        <shape>
            <corners android:radius="100dp"/>
            <gradient
                android:endColor="#F18239" android:startColor="#5B3AB7" />
        </shape>
    </item>

    <item
        android:bottom="2dp"
        android:left="2dp"
        android:right="2dp"
        android:top="2dp">

        <shape >
            <corners android:radius="100dp"/>
            <solid android:color="#fff" />
        </shape>
    </item>
</layer-list>


